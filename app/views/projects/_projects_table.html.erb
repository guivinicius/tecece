<table class="table table-striped">
  <thead>
    <tr>
      <th><%= title %></th>
    </tr>
  </thead>
  <tbody>
    <% projects.each do |p| %>
      <tr>
        <td>
          <%= p.title %>
          
        </td>
        <td>
          <%= link_to "Ver", project_path(p), :class => "btn btn-success btn-mini" %>
          <%= link_to "Editar", edit_project_path(p), :class => "btn btn-warning btn-mini" if p.user_id == current_user.id %>
        </td>
      </tr>
    <% end -%>
  </tbody>
</table>
